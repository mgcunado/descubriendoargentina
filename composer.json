{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "friendsofsymfony/rest-bundle": "2.3.1",
        "friendsofsymfony/user-bundle": "~2.0",
        "nelmio/api-doc-bundle": "^3.2",
        "sensio/framework-extra-bundle": "^5.1",
        "sonata-project/admin-bundle": "^3.33",
        "sonata-project/doctrine-orm-admin-bundle": "3.5",
        "sonata-project/easy-extends-bundle": "^2.5",
        "sonata-project/google-authenticator": "2.1",
        "sonata-project/intl-bundle": "^2.4",
        "sonata-project/seo-bundle": "^2.5",
        "sonata-project/user-bundle": "4.1.1",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/expression-language": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/google-mailer": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/mailer": "^4.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.2",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.0",
        "symfony/security-csrf": "^4.0",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/thanks": "^1.0",
        "symfony/web-server-bundle": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C1W5RJT68NZVQ2JE1M6CME33",
            "allow-contrib": false
        }
    }
}
