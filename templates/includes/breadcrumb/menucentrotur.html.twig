            {% for label, item in regiones %}
               {% if label == slug %}
                  <nav aria-label="breadcrumb">
                     <ol class="breadcrumb2">
                        <li class="breadcrumb-item"><a href="{{ path('principalescentrosturisticos') }}">Inicio</a></li>
                        <li class="breadcrumb-item"><a href="{{ path('region', {'slug': slug } ) }}">{{ item.title }}</a></li>
                        {% for labell, itemm in _context[slug]  %}
                           {% if labell == slugg %}
                              <li class="breadcrumb-item"><a href="{{ path('provincia', {'slug': slug, 'slugg': slugg } ) }}">{{ itemm.title }}</a></li>
                           {% endif %}
                        {% endfor %}

                        {% for labelll, itemmm in _context[slugg]  %}
                           {% if labelll == sluggg %}
                              <li class="breadcrumb-item"><a href="{{ path('centroturistico', {'slug': slug, 'slugg': slugg, 'sluggg': sluggg } ) }}">{{ itemmm.title }}</a></li>
                           {% endif %}
                        {% endfor %}
                              <li class="breadcrumb-item active" aria-current="page">{{ menufinal }}</li>
                     </ol>
                  </nav>
               {% endif %}
            {% endfor %}

